Effective Drytron Fafnir = {scenario = {[Drytron Fafnir] OR [Terraforming]};display = false;}

Effective Drytron Nova = {scenario = {[Drytron Nova] OR <Effective Drytron Fafnir>};display = false;}

Effective Cyber Angel Benten = {scenario = {[Cyber Angel Benten] OR [Preparation of Rites]};display = false;}

Effective Ritual = {scenario = {[Diviner of the Herald] OR [Drytron Meteonis Draconids] OR [Herald of Perfection] OR [Cyber Angel Benten] OR [Cyber Angel Idaten] OR [Cyber Angel Natasha] OR [Megalith Ophiel] OR [Megalith Phul] OR [Preparation of Rites] OR [Magikey Mechmusket - Batobuster]};display = false;}

Effective Drytron Monster = {scenario = {[Drytron Delta Altais] OR [Drytron Alpha Thuban] OR [Drytron Gamma Eltanin] OR [Drytron Zeta Aldhibah] OR [Foolish Burial]};display = false;}

Bad Draw = {scenario = {[Magikey Unlocking] OR [Dawn of the Herald] OR [Megalith Ophiel]};}

Activatable Pre-Preparation of Rites = {scenario = {NOT [Dawn of the Herald] AND [Pre-Preparation of Rites]};display = false;}

Access Mystic Mine = {scenario = {[Mystic Mine] OR [Set Rotation] OR [Terraforming]};}

Alpha + Zeta = {scenario = {COMBINATORIC{ 2;[Drytron Alpha Thuban];[Drytron Zeta Aldhibah];[Cyber Emergency];<Effective Drytron Nova>;} OR ([Foolish Burial] AND (([Drytron Zeta Aldhibah] OR [Cyber Emergency] OR <Effective Drytron Nova>) OR ([Drytron Alpha Thuban] AND <Effective Ritual>)))};}

Mu-Beta = {scenario = {(([Drytron Alpha Thuban] OR <Effective Drytron Nova>) AND ([Drytron Delta Altais] OR [Drytron Gamma Eltanin] OR [Foolish Burial])) OR (([Foolish Burial] OR [Cyber Emergency]) AND ([Drytron Delta Altais] OR [Drytron Gamma Eltanin])) OR (<Effective Ritual> AND [Drytron Gamma Eltanin] AND ([Drytron Zeta Aldhibah] OR [Drytron Delta Altais] OR [Foolish Burial]))};}

Beatrice = {scenario = {(<Effective Cyber Angel Benten> AND ([Foolish Burial] OR [Cyber Emergency] OR <Effective Drytron Nova> OR [Drytron Alpha Thuban] OR [Drytron Zeta Aldhibah])) OR ([Diviner of the Herald] AND ([Meteonis Drytron] OR (<Activatable Pre-Preparation of Rites> OR [Herald of Perfection]) OR [Cyber Angel Benten] OR [Cyber Angel Idaten] OR [Preparation of Rites]) AND ([Cyber Emergency] OR <Effective Drytron Monster> OR <Effective Drytron Nova>)) OR (<Activatable Pre-Preparation of Rites> AND ([Cyber Angel Benten] OR [Preparation of Rites]))};}

Early Perfection = {scenario = {([Cyber Angel Idaten] OR [Diviner of the Herald] OR <Effective Cyber Angel Benten>) AND (<Activatable Pre-Preparation of Rites> OR ([Dawn of the Herald] AND ([Herald of Perfection] OR [Preparation of Rites])))};}

Protection = {scenario = {[Called by the Grave] OR <Early Perfection>};}

Pre-BODE = {scenario = {<Alpha + Zeta> OR <Mu-Beta> OR <Beatrice>};}

Protected Pre-BODE = {scenario = {<Pre-BODE> AND (<Activatable Pre-Preparation of Rites> OR <Protection>&GTH&BINARY_EXPR&GTH&&GTH&CONDITION_EXPR_END&GTH&&GTH&BINARY_EXPR&GTH&&GTH&SENTINEL_END&GTH&&GTH&SEMI_COLON&GTH&&GTH&DISPLAY&GTH&&GTH&SENTINEL_END&GTH&
&GTH&MORE_SCENARIOS&GTH&&GTH&SENTINEL_END&GTH&

